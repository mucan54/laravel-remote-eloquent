{
    "name": "mucan54/laravel-remote-eloquent",
    "description": "Execute Eloquent queries on remote Laravel backend with automatic Row Level Security",
    "type": "library",
    "keywords": ["laravel", "eloquent", "remote", "api", "nativephp", "multi-tenancy", "rls"],
    "license": "MIT",
    "authors": [
        {
            "name": "mucan54",
            "email": "mucan54@example.com"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/http": "^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "RemoteEloquent\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RemoteEloquent\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RemoteEloquent\\RemoteEloquentServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
